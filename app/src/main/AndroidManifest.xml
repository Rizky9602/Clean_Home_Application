<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/logos"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logos"
        android:supportsRtl="true"
        android:theme="@style/Theme.Clean_Home"
        tools:targetApi="31">
        <activity
            android:name=".activity_cart"
            android:exported="false" />
        <activity
            android:name="com.muriz.clean_home.LoginFormActivity"
            android:exported="false" />
        <activity
            android:name="com.muriz.clean_home.MainActivity"
            android:exported="false" />
        <activity
            android:name="com.muriz.clean_home.DescriptionActivity"
            android:exported="false" />
        <activity android:name="com.muriz.clean_home.IntroActivity"
            android:exported="false"/>
        <activity
            android:name="com.MuRiz.clean_home.com.muriz.clean_home.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>